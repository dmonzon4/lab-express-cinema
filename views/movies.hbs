<div id="movies">
    <h1>Ironhack Cinema</h1>
    <h2>Click on the movie to check the showtimes!</h2>
    <br>
    <br>
    <br>
    <div class="movies-container">
        {{#each allMovies}}
        <div class="movie-item">
            <img src="{{this.image}}" alt="movie" height="200px">
            <p>{{this.title}}</p>
            <p>
                <a href="/movie/{{this._id}}">See more</a>
            </p>
        </div>
        {{/each}}
    </div>
</div>